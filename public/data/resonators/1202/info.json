"lv1": "", "lv2": "", "lv3": "", "lv4": "", "lv5": "", "lv6": "", "lv7": "", "lv8": "", "lv9": "", "lv10": "", "_resonance_energy": "", "_concerto_energy": "", "_hardness": "", "_toughness": "", "sta_regen": "", "resonance_energy_regen": "", "concerto_regen": "10.00", "sta_cost": "", "concerto_cost": "", "resonance_cost": "", "cd": "", "duration": ""}, {"id": "躍焰", "type": "Damage", "skill_type": "延奏技能", "element": "熱熔", "base_attr": "攻擊", "bonus_type": "延奏", "bonus_type_2": "", "coordinated": "", "lv1": "530.00%", "lv2": "530.00%", "lv3": "530.00%", "lv4": "530.00%", "lv5": "530.00%", "lv6": "530.00%", "lv7": "530.00%", "lv8": "530.00%", "lv9": "530.00%", "lv10": "530.00%", "_resonance_energy": "", "_concerto_energy": "", "_hardness": "", "_toughness": "", "sta_regen": "", "resonance_energy_regen": "", "concerto_regen": "", "sta_cost": "", "concerto_cost": "", "resonance_cost": "", "cd": "", "duration": ""}], "skill_infos": {"normal_attack": {"name": "砰砰", "description": "普攻\n進行最多4段連續射擊，造成熱熔傷害。\n\n重擊\n進入瞄準狀態，可以進行更強力的射擊。\n蓄力完成後攻擊目標，造成熱熔傷害。\n\n空中攻擊\n消耗耐力，在空中進行連續攻擊，造成熱熔傷害。\n\n閃避反擊\n成功閃避後一定時間內短按普攻，將會攻擊目標，造成熱熔傷害。"}, "resonance_skill": {"name": "咻咻斗意", "description": "燃起鬥志對目標進行高速連射，造成熱熔傷害。\n\n咻咻斗意初始擁有2次可使用次數。"}, "forte_circuit": {"name": "英魂熾火流", "description": "共鳴技能·喀喀壓制\n長按共鳴技能咻咻斗意，進入喀喀壓制狀態，在該狀態下：\n·持續消耗【熱壓彈】攻擊目標，造成熱熔傷害，此次傷害為共鳴技能傷害；\n·短按普攻，將會施放第4段普攻攻擊目標，造成熱熔傷害，此次傷害為普攻傷害，並退出喀喀壓制；\n·若短按普攻時已消耗30發【熱壓彈】，將會施放共鳴技能轟轟，並退出喀喀壓制；\n·當【熱壓彈】消耗完畢後，退出喀喀壓制。\n\n共鳴技能·轟轟\n造成熱熔傷害，此次傷害為共鳴技能傷害。\n\n熱壓彈獲取規則\n【熱壓彈】容量上限60發。\n固有技能灼熱彈匣可將【熱壓彈】容量上限擴充10發。\n常態攻擊砰砰命中目標時，可積攢【熱壓彈】。\n施放變奏技能堂堂登場時，可積攢【熱壓彈】。\n施放共鳴技能咻咻斗意時，可積攢【熱壓彈】。"}, "resonance_liberation": {"name": "熾烈焰火", "description": "體內熱流湧現，對附近目標進行快速射擊，造成熱熔傷害。"}, "intro_skill": {"name": "堂堂登場", "description": "颯爽登場，對目標進行快速射擊，造成熱熔傷害。"}, "outro_skill": {"name": "躍焰", "description": "以技能目標為中心，釋放衝擊波，對範圍內的目標造成熾霞530.00%攻擊的熱熔傷害。"}, "inherent_skill_1": {"name": "灼熱彈匣", "description": "【熱壓彈】容量上限擴充10發，共鳴技能轟轟傷害提升50%。"}, "inherent_skill_2": {"name": "加麻加辣", "description": "共鳴技能喀喀壓制持續期間，每發【熱壓彈】命中可使攻擊提升1%，持續10秒，可疊加30層。"}, "chain1": {"name": "劇院的英雄戲", "description": "施放共鳴技能轟轟時，必定暴擊。"}, "chain2": {"name": "躍動的火星", "description": "施放共鳴解放熾烈焰火期間，每擊敗1個目標，回復5點共鳴能量，每次最多可回復20點。"}, "chain3": {"name": "不滅的火把", "description": "共鳴解放熾烈焰火對生命值低於50%的目標，傷害提升40%。"}, "chain4": {"name": "英雄的絕招", "description": "施放共鳴解放熾烈焰火時，熾霞獲得60發【熱壓彈】，並立即重置一次共鳴技能咻咻斗意的冷卻時間。"}, "chain5": {"name": "勝利的槍彈焰火", "description": "固有技能加麻加辣疊加至滿層時，攻擊額外提升30%。"}, "chain6": {"name": "劇終的回歸彩蛋", "description": "觸發共鳴技能轟轟後，隊伍中的角色普攻傷害加成提升25%，持續15秒。"}}}