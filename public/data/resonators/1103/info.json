inated": "", "lv1": "", "lv2": "", "lv3": "", "lv4": "", "lv5": "", "lv6": "", "lv7": "", "lv8": "", "lv9": "", "lv10": "", "_resonance_energy": "", "_concerto_energy": "", "_hardness": "", "_toughness": "", "sta_regen": "", "resonance_energy_regen": "", "concerto_regen": "4.00", "sta_cost": "", "concerto_cost": "", "resonance_cost": "", "cd": "", "duration": ""}, {"id": "共鳴技能消耗【念意】額外回復協奏能量", "type": "", "skill_type": "共鳴回路", "element": "", "base_attr": "", "bonus_type": "", "coordinated": "", "lv1": "", "lv2": "", "lv3": "", "lv4": "", "lv5": "", "lv6": "", "lv7": "", "lv8": "", "lv9": "", "lv10": "", "_resonance_energy": "", "_concerto_energy": "", "_hardness": "", "_toughness": "", "sta_regen": "", "resonance_energy_regen": "", "concerto_regen": "8.00", "sta_cost": "", "concerto_cost": "", "resonance_cost": "", "cd": "", "duration": ""}, {"id": "重擊消耗【念意】額外回復共鳴能量", "type": "", "skill_type": "共鳴回路", "element": "", "base_attr": "", "bonus_type": "", "coordinated": "", "lv1": "", "lv2": "", "lv3": "", "lv4": "", "lv5": "", "lv6": "", "lv7": "", "lv8": "", "lv9": "", "lv10": "", "_resonance_energy": "", "_concerto_energy": "", "_hardness": "", "_toughness": "", "sta_regen": "", "resonance_energy_regen": "", "concerto_regen": "2.50", "sta_cost": "", "concerto_cost": "", "resonance_cost": "", "cd": "", "duration": ""}], "skill_infos": {"normal_attack": {"name": "應許", "description": "普攻\n指揮憂曇進行最多4段的連續攻擊，造成冷凝傷害。\n\n重擊\n持續消耗耐力，牽引憂曇攻擊目標，造成冷凝傷害。重擊期間可控制憂曇進行移動。\n\n空中攻擊\n消耗耐力，在空中召喚憂曇進行下落攻擊，造成冷凝傷害。\n\n閃避反擊\n成功閃避後一定時間內短按普攻，將會攻擊目標，造成冷凝傷害。"}, "resonance_skill": {"name": "應急預案", "description": "呼喚憂曇攻擊目標，造成冷凝傷害，並立即為附近隊伍中所有角色回復生命值。"}, "chain1": {"name": "極簡與繁複", "description": "施放共鳴技能應急預案時，每消耗1點【念意】，白芷額外回復2.5點共鳴能量。"}, "resonance_liberation": {"name": "剎那合彌", "description": "召喚憂曇瞬間為附近隊伍中所有角色回復生命值，並依次產生4段頻隙迴響。\n\n頻隙迴響\n頻隙迴響會跟隨隊伍中登場角色，每2.5秒會消耗1段頻隙迴響進行協同攻擊，命中目標時造成冷凝傷害，並以命中點為中心，為附近隊伍中所有角色回復生命值。"}, "intro_skill": {"name": "覆雪流盈", "description": "白芷召喚憂曇，進行1次俯衝攻擊，造成冷凝傷害，並為附近隊伍中所有角色回復生命值。"}, "outro_skill": {"name": "命源輸送", "description": "持續為下一位登場角色回復生命值，每3秒回復白芷1.54%生命上限的生命值，持續30秒。受到此治療效果時，角色全傷害加深15%，持續6秒。"}, "inherent_skill_1": {"name": "泛音區間", "description": "施放共鳴技能應急預案時，憂曇會留下一片天籟，存在時間15秒。\n\n天籟\n拾取天籟的角色攻擊提升15%，持續20秒。"}, "inherent_skill_2": {"name": "激策回饋", "description": "重擊每次命中目標時，都能為附近隊伍中生命值最少的角色回復生命值，回復量為白芷自身0.25%生命上限。"}, "chain2": {"name": "沉默的冰原", "description": "施放共鳴技能應急預案時，若有4點【念意】，白芷的冷凝傷害加成提升15%，治療效果加成提升15%，持續12秒。"}, "chain3": {"name": "真理的崇奉", "description": "施放變奏技能覆雪流盈時，白芷的生命上限提升12%，持續10秒。"}, "chain4": {"name": "被追溯的本源", "description": "施放共鳴解放剎那合彌時，共鳴解放頻隙迴響獲得強化：\n·頻隙迴響增加2段；\n·頻隙迴響治療倍率提升20%；\n·頻隙迴響額外造成白芷1.20%生命值上限的冷凝傷害。"}, "chain5": {"name": "被回應的祈願", "description": "隊伍中的白芷存活時，若隊伍中的角色（不包含白芷）失去意識，立刻為該角色恢復意識並回復該角色生命上限100%的生命值，該效果每10分鐘可觸發1次。"}, "chain6": {"name": "聞道者的覺悟", "description": "角色拾取固有技能天籟時，附近隊伍中所有角色的冷凝傷害加成提升12%，持續20秒。"}}}