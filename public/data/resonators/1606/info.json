, "lv2": "311.62%", "lv3": "335.24%", "lv4": "368.30%", "lv5": "391.92%", "lv6": "419.07%", "lv7": "456.86%", "lv8": "494.64%", "lv9": "494.64%", "lv10": "572.58%", "_resonance_energy": "1.20", "_concerto_energy": "0.00", "_hardness": "1.00", "_toughness": "1.0000", "sta_regen": "", "resonance_energy_regen": "1.20", "concerto_regen": "0.00", "sta_cost": "", "concerto_cost": "", "resonance_cost": "", "cd": "", "duration": ""}, {"id": "普攻·幻想照進現實-第一段傷害", "type": "Damage", "skill_type": "共鳴回路", "element": "湮滅", "base_attr": "攻擊", "bonus_type": "共鳴技能", "bonus_type_2": "", "coordinated": "", "lv1": "162.00%", "lv2": "175.29%", "lv3": "188.57%", "lv4": "207.17%", "lv5": "220.45%", "lv6": "235.73%", "lv7": "256.99%", "lv8": "278.24%", "lv9": "278.24%", "lv10": "322.08%", "_resonance_energy": "8.00", "_concerto_energy": "0.00", "_hardness": "1.00", "_toughness": "1.0000", "sta_regen": "", "resonance_energy_regen": "8.00", "concerto_regen": "17.00", "sta_cost": "", "concerto_cost": "", "resonance_cost": "", "cd": "", "duration": ""}, {"id": "普攻·幻想照進現實-第二段傷害", "type": "Damage", "skill_type": "共鳴回路", "element": "湮滅", "base_attr": "攻擊", "bonus_type": "共鳴技能", "bonus_type_2": "", "coordinated": "", "lv1": "171.00%", "lv2": "185.03%", "lv3": "199.05%", "lv4": "218.68%", "lv5": "232.70%", "lv6": "248.83%", "lv7": "271.26%", "lv8": "293.70%", "lv9": "293.70%", "lv10": "339.97%", "_resonance_energy": "8.00", "_concerto_energy": "0.00", "_hardness": "1.00", "_toughness": "1.0000", "sta_regen": "", "resonance_energy_regen": "8.00", "concerto_regen": "17.00", "sta_cost": "", "concerto_cost": "", "resonance_cost": "", "cd": "", "duration": ""}, {"id": "普攻·幻想照進現實-第三段傷害", "type": "Damage", "skill_type": "共鳴回路", "element": "湮滅", "base_attr": "攻擊", "bonus_type": "共鳴技能", "bonus_type_2": "", "coordinated": "", "lv1": "180.00%", "lv2": "194.76%", "lv3": "209.52%", "lv4": "230.19%", "lv5": "244.95%", "lv6": "261.92%", "lv7": "285.54%", "lv8": "309.15%", "lv9": "309.15%", "lv10": "357.86%", "_resonance_energy": "8.00", "_concerto_energy": "0.00", "_hardness": "1.00", "_toughness": "1.0000", "sta_regen": "", "resonance_energy_regen": "8.00", "concerto_regen": "17.00", "sta_cost": "", "concerto_cost": "", "resonance_cost": "", "cd": "", "duration": ""}], "skill_infos": {"normal_attack": {"name": "佩洛，悠著點", "description": "普攻\n進行最多4段的連續攻擊，造成湮滅傷害。\n\n重擊\n消耗耐力，攻擊目標，造成湮滅傷害，命中目標時若【想像力】至少有100點，則將洛可可送上空中，進入飛躍幻想狀態。可通過持續按住普攻進行蓄力，蓄力越久能獲得越多的【想像力】，蓄力期間若耐力耗盡亦可繼續蓄力。蓄力中途鬆開普攻，或者達到蓄力的最大時間時，會自動施放重擊。\n\n空中攻擊\n消耗耐力進行空中下落攻擊，造成湮滅傷害。\n\n閃避反擊\n成功閃避後一定時間內短按普攻，將會攻擊目標，造成湮滅傷害。"}, "resonance_skill": {"name": "高難度設計", "description": "洛可可投影出自己腦中的思維風暴，牽引附近目標，造成湮滅傷害，並且把洛可可送上空中，進入飛躍幻想狀態。"}, "forte_circuit": {"name": "道具師的自我修養", "description": "飛躍幻想\n·洛可可處於飛躍幻想狀態時，若至少擁有100點【想像力】，可短按普攻，消耗100點【想像力】施放普攻幻想照進現實。\n·該狀態會在洛可可不處於空中狀態、或處於後台時解除。\n\n普攻·幻想照進現實\n·進行最多3段的連續攻擊，造成湮滅傷害，此次傷害為重擊傷害。第1段、第2段攻擊落地後，若【想像力】大於100點，會將洛可可送上空中，進入飛躍幻想狀態。\n\n【想像力】獲取規則\n·【想像力】上限300點\n·常態攻擊造成傷害可回復【想像力】。\n·持續按住普攻進行蓄力可獲得【想像力】。\n·施放共鳴技能高難度設計時，回復100點【想像力】\n·施放變奏技能佩洛，來幫忙時，回復100點【想像力】。"}, "resonance_liberation": {"name": "即興喜劇，開場", "description": "洛可可的即興喜劇開場！對目標造成湮滅傷害，此次傷害為重擊傷害。洛可可暴擊高於50%時，每多出0.1%暴擊，施放該技能時，使隊伍中的角色攻擊提升1點，持續30秒。最高可提升200點。"}, "intro_skill": {"name": "佩洛，來幫忙", "description": "攻擊目標，造成湮滅傷害。施放該技能後在一定時間內短按普攻，將會施放普攻第4段。"}, "outro_skill": {"name": "掌聲鼓勵", "description": "下一位登場角色湮滅傷害加深20%，普攻傷害加深25%，效果持續14秒，若切換至其他角色則該效果提前結束。"}, "inherent_skill_1": {"name": "沉浸式演出", "description": "施放共鳴技能或重擊時，洛可可攻擊提升20%，持續12秒。"}, "inherent_skill_2": {"name": "無盡引力珍奇箱", "description": "施放延奏技能後，下一個登場角色的探索工具，替換為【珍奇箱】。\n\n【珍奇箱】\n·使用後以【珍奇箱】為中心牽引周圍目標，造成共計100點的湮滅傷害，此次傷害為探索工具傷害。該技能施放時，視為施放聲骸技能。\n·【珍奇箱】持續14秒，若切換至其他角色則【珍奇箱】提前消失。"}, "chain1": {"name": "沉悶的灰暗湧進船艙", "description": "施放共鳴技能高難度設計時，額外回復100點【想像力】與10點協奏能量。\n普攻幻想照進現實免疫打斷。"}, "chain2": {"name": "海螢石閃爍著微弱光芒", "description": "施放普攻幻想照進現實時，隊伍中的角色湮滅傷害加成提升10%，可疊加3層，持續30秒。滿層時，隊伍中的角色湮滅傷害加成額外提升10%，持續30秒。"}, "chain3": {"name": "用心觀察，以手丈量", "description": "施放變奏技能佩洛，來幫忙時，洛可可暴擊提升10%，暴擊傷害提升30%，持續15秒。"}, "chain4": {"name": "千萬“奇藏”於箱中匯聚", "description": "施放共鳴技能高難度設計時，普攻幻想照進現實傷害倍率提升60%，持續12秒。"}, "chain5": {"name": "重建樂土，在舞台上", "description": "共鳴解放即興喜劇，開場傷害倍率提升20%，重擊傷害倍率提升80%。"}, "chain6": {"name": "飛吧，乘著金色的翅膀", "description": "施放共鳴解放即興喜劇，開場時，獲得如下效果，12秒內：\n·普攻幻想照進現實攻擊目標時，無視對方60%的防禦。\n·普攻幻想照進現實第3段落地後，會將洛可可送上空中，進入飛躍幻想狀態，此時短按普攻，將會施放普攻構築現實，造成普攻幻想照進現實第3段100%的傷害，此次傷害為重擊傷害。\n·普攻構築現實落地後，會將洛可可送上空中，進入飛躍幻想狀態。普攻構築現實需要在處於飛躍幻想狀態時才可施放。\n·普攻構築現實免疫打斷。"}}}